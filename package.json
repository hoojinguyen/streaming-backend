  "scripts": {
    "start": "better-npm-run start:production",
    "dev": "nodemon -e ts -w ./ --delay 1500ms -x better-npm-run start:dev",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "start:production": {
      "command": "node -r ts-node/register -r tsconfig-paths/register ./src/streaming-backend.ts",
      "env": {
        "DEBUG": "*",
        "NODE_ENV": "production",
        "USE_MOCK_SERVICE": "false"
      }
    },
    "start:dev": {
      "command": "node -r ts-node/register -r tsconfig-paths/register ./src/streaming-backend.ts",
      "env": {
        "DEBUG": "streaming:*",
        "NODE_ENV": "development",
        "USE_MOCK_SERVICE": "false"
      }
    },
    "test": {
      "command": "nyc ava --serial ./test/execute.js",
      "env": {
        "DEBUG": "streaming:*",
        "NODE_ENV": "development",
        "USE_MOCK_SERVICE": "true"
      }
    }
  }
